    MIT            MITLicense	
    LGPL-2.1-only  GNU Lesser General Public License v2.1 only
    LGPL-3.0-only  GNU Lesser General Public License v3.0 only	 
    ...and more...
	-->
  <metadata>
        <id>$id$</id>
        <version>$version$</version>
        <title>$title$</title>
        <authors>$author$</authors>
        <owners>$author$</owners>
        <license type="expression">MIT</license>
        <projectUrl>https://github.com/$author$/$id$</projectUrl>
        <iconUrl>https://raw.githubusercontent.com/$author$/$id$/master/icon.png</iconUrl>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <description>$description$</description>
		<developmentDependency>false</developmentDependency>
        <releaseNotes>https://raw.githubusercontent.com/$author$/$id$/master/ChangeLog.md</releaseNotes>
        <copyright>Copyright 2019 by $author$. All rights reserved.</copyright>
        <tags>example $author$</tags>
    </metadata>
</package>